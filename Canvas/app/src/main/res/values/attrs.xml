<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VerticalView">
        <attr name="pointer" format="reference"/>
        <attr name="bg_top" format="reference"/>
    </declare-styleable>

    <declare-styleable name="UpAndDownView">
        <attr name="bg" format="reference"/>
        <attr name="image_scale_type">
            <enum name="fillXY" value="0"/>
            <enum name="center" value="1"/>
        </attr>
    </declare-styleable>
</resources>
